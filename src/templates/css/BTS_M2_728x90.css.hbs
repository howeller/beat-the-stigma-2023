#banner * {
	font-weight: 700;
	text-align: center;
}

/* Frame1 */

#bts-logo {
	width: 148px;
  top: 22px;
  left: 291px;
}

.bg-blend {
	background-color: rgba(255, 7, 7, 0.5);
	background-blend-mode: color;
}

.game-wrapper, svg#header-back  {
}

svg#header-back {
	width: 223px;
	height: auto;
	overflow: visible;
	margin: 18px 12px 0;
	{{~#if (showFpo)}}/*opacity:0.1;*/{{/if~}} 
}

.game-wrapper {
  width: 100%;
  height: 90px;
}

.game-txt {
	font-size: 14px;
	line-height: 15px;
  /* letter-spacing: -1px; */
	{{~#if (showFpo)}}color: green !important;{{/if~}}
}
p.header-txt {
  display: inline-block;
  position: absolute;
	width: 223px;
  left: 12px;
  top: 38px;
}

ol {
	display: flex;
  flex-direction: row;
	position: absolute;
	/* overflow: hidden; */
	width: 100%;
  top: 25px;
  left: 241px;
	/* margin-top: 1px; */
}

li {
	width: 113px;
	height: 58px;
	border-radius: 3px;
	box-sizing: border-box;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
	display: inline-flex;
	align-items: center;
	justify-content: center;
	margin-right: 7px;
}

#game1-wrapper li {
	border: solid {{borderStroke}}px {{frame2.borderRed}};
}
#game2-wrapper li {
	border: solid {{borderStroke}}px #84bfe6;
}

#g1-answers li {
	background-color: rgba(75, 19, 47, {{frame2.fillOpacity}});
	/* background-color: rgba(79, 19, 47, 0.7); */
}
#g2-answers li {
	background-color: {{group.blueFill}};
}
#g1-wrong-answers li span.strikethrough {
	position: relative;
  {{~#if (showFpo)}}color: orange;{{/if}}
}

span.strike-wrapper {
  display: inline-block;
}

#g1-wrong-answers li span.strikethrough::after {
	position: absolute;
  /* display: block; */
	border-top: 1px solid white;
	background-color: white;
	content: ' ';
	left: 0;
	top: 50%;
	width: calc(100% + 3px);
	margin: 0px -2px;
}

/* Endframe */

#end-container  * {
	position: relative !important;
}

#end-container  {
	/* position: relative !important; */
	display: flex;
  align-items: center;
  /* justify-content: center; */
  flex-direction: row;
}

#end-txt{
	font-size: 19px;
	line-height: 12px;
  text-align: left;
  letter-spacing: -0.2px;
	/* margin: 0 56px 30px; */
  margin-left: 56px;
  margin-right: 33px;
	{{~#if (showFpo)}}color: red;{{/if}}
}

#cta {
	background-color: var(--teal);
	border-radius: 4px;
	color: white;
	display: inline-flex;
	align-items: center;
	justify-content: center;
	margin-right: 40px;
	padding: 3px 10px;
	font-size: 13px;
	letter-spacing: -0.5px;

	{{~#if (showFpo)}}visibility: visible; background-color: rgba(255, 0, 0, 0.4); color:red{{/if}}
}

#cta-txt {
	white-space: nowrap;
	width: auto;
}

svg#ohio-logo{
	width: 37px;
}