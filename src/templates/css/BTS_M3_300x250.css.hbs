#banner * {
	font-weight: 700;
	text-align: center;
}

/* Frame1 */

#bts-logo {
	width: 153px;
	margin: 102px auto 0;
	position: relative;
}

.game-wrapper/* , svg#header-back */  {
	margin: 166px 39px 0;
	width: 224px;
	overflow: visible;
}

svg#header-back {
	width: 100%;
	height: auto;
	overflow: visible;
	{{~#if (showFpo)}}opacity:0.5;{{/if~}}
}
.game-txt {
	font-size: 12px;
	line-height: 15px ;
	letter-spacing: -.1px;
	{{~#if (showFpo)}}color: red !important;{{/if~}}
}

#txt-wrapper {
	display: flex;
	align-items: center;
	justify-content: center;
	width: 100%;
	height: 45px;
	margin-top: 12px;
}

/* Endframe */

#end-wrapper * {
	position: relative !important;
	display: block;
}

#end-txt{
	font-size: 17px;
	line-height: 18px;
	margin-top: 96px;
	{{~#if (showFpo)}}color: red;{{/if}}
}

#cta {
	background-color: var(--teal);
	border-radius: 5px;
	color: white;
	display: flex;
	align-items: center;
	justify-content: center;
	margin: 17px auto 0;
	padding: 3px 7px;
	font-size: 15px;
	letter-spacing: -0.5px;
	{{~#if (showFpo)}}visibility: visible;background-color: rgba(255, 0, 0, 0.4); color:red{{/if}}
}

#cta-txt {
	white-space: nowrap;
	width: auto;
}

svg#ohio-logo{
	width: 28px;
	margin: 20px auto;
}