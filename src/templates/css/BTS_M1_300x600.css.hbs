/* Frame2 */

div.letter-line-container {
	display: flex;
	justify-content: center;
	width: 100%;
}
#f2-letter-line1 {
	margin-top: 202px;
}
#f2-letter-line2 {
	margin-top: 12px;
}

/* F2 Individual letters */
div.letter-box {
	background-color: var(--teal);
	border-radius: 2px;
	display: inline-flex;
	justify-content: center;
	align-items: center;
	text-align: center;
	width: 24px;
	height: 34px;
	margin: 0 6px;
	font-size: 30px;
	{{#if (showFpo)}}background-color: rgba(255, 0, 0, 0.4); color:black !important;{{/if}}
}

p#f2-txt {
	font-size: 18px;
	line-height: 1.1;
	margin-top: 40px;
	{{#if (showFpo)}}color: red;{{/if}}
}

svg#outline {
	top: 143px;
	left: 19px;
	width: 260px;
	object-fit: contain;
}

/* Endframe */

#end-container * {
	position: relative !important;
	display: block;
}

#end-txt{
	font-size: 29px;
	line-height: 30px;
	margin-top: 196px;
	{{#if (showFpo)}}color: red;{{/if}}
}

/* CTA Btn */

#cta {
	background-color: var(--teal);
	border-radius: 5px;
	display: flex;
	align-items: center;
	justify-content: center;
	margin: 31px auto 0;
	padding: 8px 12px;
	font-size: 16px;
	letter-spacing: -0.5px;
	{{#if (showFpo)}}background-color: rgba(255, 0, 0, 0.4); color:red;{{/if}}
}
#cta-txt {

	white-space: nowrap;
	width: auto;
}

svg#ohio-logo{
	width: 52px;
	margin: 146px auto 0;
}
