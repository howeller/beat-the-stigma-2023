#end-txt{
	color: var(--teal);
	font-weight: 700;
	font-size: 42px;
	line-height: 100%;
	margin: 142px 0px 32px 28px;
}

/* Frame2 Quiz */

div#f2-container h1{
	color: var(--teal);
	font-size: 42px;
	line-height: 100%;
	margin: 114px 0px 12px 28px;
}

ol.f2-txt{
	color: white;
	font-weight: 600;
	font-size: 25px;
	line-height: 28px;
}

/* F2 Individual lines */

ol.f2-txt li {
	display: list-item;
	list-style-position: inside;
	padding: 2px 8px;
	margin: 2px 0px 0px 22px;
}

/* The highlighted lines */

li.highlight-txt{
	background: linear-gradient(to right, var(--teal) 49.9%, rgba(0, 0, 0, 0.0) 49.9%);
	background-size: 200% 100%;
	background-position: right bottom;
	overflow: hidden;
	border-radius: 3px;
	width: -moz-fit-content;
	width: fit-content;
}

span.line-indent{
	padding-left: 28px;
	margin-top: -2px;
}
