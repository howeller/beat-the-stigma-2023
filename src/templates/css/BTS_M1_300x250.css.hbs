/* Frame2 */

div.letter-line-container {
	display: flex;
	justify-content: center;
	width: 100%;
}
#f2-letter-line1 {
	margin-top: 94px;
}
#f2-letter-line2, p#f2-txt {
	margin-top: 5px;
}

/* F2 Individual letters */
div.letter-box {
	background-color: var(--teal);
	/* background-color: rgba(255, 0, 0, 0.2); */
	border-radius: 2px;
	display: inline-flex;
	justify-content: center;
	align-items: center;
	text-align: center;
	width: 17px;
	height: 22px;
	margin: 0 3px;
	/* padding: 2px; */
	font-size: 19px;
}

p#f2-txt {
	font-size: 11px;
	line-height: 1.1;
	{{#if (showFpo)}}color: red;{{/if}}
}

svg#outline {
	top: 69px;
	left: 24px;
	width: 249px;
}

/* Endframe */

#end-container * {
	position: relative !important;
	display: block;
}

#end-txt{
	font-size: 17px;
	margin-top: 96px;
	{{#if (showFpo)}}color: red;{{/if}}
}

/* CTA Btn */

#cta {
	background-color: var(--teal);
	/* background-color: rgba(255, 0, 0, 0.4); */
	border-radius: 5px;
	display: flex;
	align-items: center;
	justify-content: center;
	margin: 20px auto 0;
	padding: 5px 7px;
}

#cta-txt {
	font-size: 15px;
	/* line-height: 22px; */
	letter-spacing: -0.5px;
	white-space: nowrap;
	width: auto;
}

svg#ohio-logo{
	width: 28px;
	margin: 20px auto;
}







