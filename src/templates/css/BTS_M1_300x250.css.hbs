div.letter-line-container, p.f2-txt {
	display: block;
}

p.f2-txt {
	font-weight: 600;
	font-size: 21px;
	line-height: 1.1;
	text-align: center;
}

/* F2 Individual letters */
div.letter-box {
	background-color: var(--teal);
	padding: 25px 25px 10px;
}

div.letter-box span {
	text-align: center;
	font-size: 25px;
	line-height: 1;
	display: inline-block;
}
