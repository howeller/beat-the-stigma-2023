#banner * {
	font-weight: 700;
	text-align: center;
}

/* Frame1 */

img#bts-logo {
	width: 229px;
	margin: 249px auto 0;
	position: relative;
}

.game-wrapper {
	margin: 475px 22px 0;
	width: 256px;
	overflow: visible;
}

svg#header-back {
	width: 100%;
	height: auto;
	overflow: visible;
	{{~#if (showFpo)}}opacity:0.6;{{/if~}}
}
.game-txt {
	font-size: 17px;
	line-height: 18px;
	/* letter-spacing: -.1px; */
	{{~#if (showFpo)}}color: red !important;{{/if~}}
}

#txt-wrapper {
	display: flex;
	align-items: center;
	justify-content: center;
	width: 100%;
	height: 86px;
	margin-top: 17px;
}

/* Endframe */

#end-wrapper * {
	position: relative !important;
	display: block;
}

#end-txt{
	font-size: 29px;
	line-height: 30px;
	margin-top: 195px;
	{{~#if (showFpo)}}color: red;{{/if}}
}

#cta {
	background-color: var(--teal);
	border-radius: 4px;
	color: white;
	display: flex;
	align-items: center;
	justify-content: center;
	margin: 32px auto 0;
	padding: 6px 12px;
	font-size: 16px;
	letter-spacing: -0.5px;
	{{~#if (showFpo)}}visibility: visible;background-color: rgba(255, 0, 0, 0.4); color:red{{/if}}
}

#cta-txt {
	white-space: nowrap;
	width: auto;
}

svg#ohio-logo{
	width: 56px;
	margin: 144px auto 0;
}