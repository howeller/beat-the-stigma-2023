#banner * {
	font-weight: 700;
	text-align: center;
}

/* Frame1 */

img#bts-logo {
	width: 148px;
  top: 22px;
  left: 291px;
	/* position: relative; */
}

.game-wrapper {
	margin: 19px 416px 0;
	width: 262px;
	overflow: visible;
}

svg#header-back {
	width: 100%;
	height: auto;
	overflow: visible;
	{{~#if (showFpo)}}opacity:0.6;{{/if~}}
}
.game-txt {
	font-size: 14px;
	line-height: 15px;
	/* letter-spacing: -.1px; */
	{{~#if (showFpo)}}color: red !important;{{/if~}}
}

#txt-wrapper {
	display: flex;
	align-items: center;
	justify-content: center;
	width: 100%;
	height: 48px;
	margin-top: 10px;
}

/* Endframe */

#end-wrapper  * {
	position: relative;
}

#end-wrapper  {
	display: flex;
  align-items: center;
  flex-direction: row;
}

#end-txt{
	font-size: 19px;
	line-height: 12px;
  text-align: left;
  letter-spacing: -0.2px;
  margin-left: 56px;
  margin-right: 33px;
	{{~#if (showFpo)}}color: red;{{/if}}
}

#cta {
	background-color: var(--teal);
	border-radius: 4px;
	color: white;
	display: inline-flex;
	align-items: center;
	justify-content: center;
	margin-right: 40px;
	padding: 3px 10px;
	font-size: 13px;
	letter-spacing: -0.5px;

	{{~#if (showFpo)}}visibility: visible; background-color: rgba(255, 0, 0, 0.4); color:red{{/if}}
}

#cta-txt {
	white-space: nowrap;
	width: auto;
}

svg#ohio-logo{
	width: 37px;
}