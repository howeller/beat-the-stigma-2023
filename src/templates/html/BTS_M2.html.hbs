<!-- 
	WRAP THE TXT INSIDE A IF YOU NEED TO CONTROL THE SPACE BETWEEN MARKER AND TXT
	{{#unless @last}}<li class="wrong-answer"><span>{{{.}}}</span></li>{{else}}<li id="answer" class="highlight-txt"><span>{{{.}}}</span></li>{{/unless}}{{/each}} -->--}}
</ol>
<!-- Duplicate list of strike throughs -->
<ol id="f2-strike" class="f2-txt" type="A" aria-hidden="true">{{#each frame2.strikeList}}
